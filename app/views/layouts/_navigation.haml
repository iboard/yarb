.navbar.navbar-fixed-top
  .navbar-inner
    .container
      = render 'layouts/collapsed_menu_bars'
      =link_to 'YARB', root_path, class: 'brand'
      .nav-collapse.collapse
        %ul.nav
          %li{class: active_class(root_path)}= link_to t(:home), root_path
          %li{class: active_class(pages_path)}= link_to t(:pages), pages_path
        %ul.nav.pull-right
          = render 'session/session_nav'
