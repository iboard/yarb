.navbar.navbar-fixed-top
  .navbar-inner
    .container
      = render 'layouts/collapsed_menu_bars'
      =link_to 'YARB', root_path, class: 'brand'
      .nav-collapse.collapse
        %ul.nav
          %li{class: current_page?(root_path)  ? 'active' : ''}= link_to t(:home), root_path
          %li{class: current_page?(pages_path) ? 'active' : ''}= link_to t(:pages), pages_path
        %ul.nav.pull-right
          = render 'session/session_nav'

